    <SolidColorBrush x:Key="ColorBrush1" Color="Transparent" />
    <SolidColorBrush x:Key="ColorBrush2" Color="{Binding Color, Source={StaticResource ZBMSAccentColorBrush}}" />
    <SolidColorBrush x:Key="ColorBrush5" Color="LightGray" />

    <SolidColorBrush x:Key="ColorBrush3" Color="{Binding Color, Source={StaticResource ZBMSAccentColorBrush}}" />
    <SolidColorBrush x:Key="ColorBrush4" Color="White" />
    <SolidColorBrush x:Key="ColorBrush6" Color="Black" />
    <SolidColorBrush x:Key="ColorBrush7" Color="Red" />

    <AcrylicBrush
        x:Key="AcrylicBrush1"
        BackgroundSource="Backdrop"
        FallbackColor="White"
        TintColor="White"
        TintOpacity="0.2" />
    <AcrylicBrush
        x:Key="AcrylicBrush2"
        BackgroundSource="Backdrop"
        TintColor="{Binding Color, Source={StaticResource ZBMSAccentColorBrush}}"
        TintOpacity="0.5" />


    <SolidColorBrush x:Key="ThemeColor" Color="{Binding Color, Source={StaticResource ZBMSAccentColorBrushLight1}}" />
